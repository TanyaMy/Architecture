<Page
    x:Class="Architecture.Presentation.Views.ArchitectureMainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="using:Architecture.Presentation.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    DataContext="{Binding ArchitectureMain, Source={StaticResource Locator}}"
    mc:Ignorable="d">

    <Page.BottomAppBar>
        <AppBar ClosedDisplayMode="Compact">
            <StackPanel Orientation="Horizontal">
                <AppBarButton Icon="Pin" />
                <AppBarButton Icon="Pin" />
                <AppBarButton Icon="Pin" />
                <AppBarButton Icon="Pin" />
                <AppBarButton Icon="Pin" />
            </StackPanel>
        </AppBar>
    </Page.BottomAppBar>


    <Frame Name="ArchitectureFrame" />
</Page>
